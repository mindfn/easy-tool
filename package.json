{
  "name": "nuxt-typescript-template",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql-import": "^0.6.0",
    "graphql-yoga": "^1.16.2",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "moment": "^2.22.2",
    "mongoose": "^5.2.12",
    "muse-ui": "^3.0.0",
    "muse-ui-loading": "^0.2.0",
    "muse-ui-toast": "^0.3.0",
    "ntypescript": "1.201706190042.1",
    "nuxt": "^1.3.0",
    "nuxt-property-decorator": "^1.2.0",
    "nuxt-start": "^1.0.0-rc11",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "scripts": {
    "build": "nuxt build && backpack build",
    "pro": "cross-env NODE_ENV=production node build/main.js",
    "dev:client": "cross-env NODE_ENV=development DEV_TYPE=nuxt ts-node --compiler ntypescript --project tsconfig_node.json ./server",
    "dev:server": "cross-env NODE_ENV=development DEV_TYPE=server ts-node-dev --compiler ntypescript --project tsconfig_node.json ./server",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commitmsg": "validate-commit-msg"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/mongodb": "^3.1.4",
    "@types/mongoose": "^5.2.9",
    "@types/node": "^9.4.6",
    "backpack-core": "^0.7.0",
    "conventional-changelog": "^2.0.3",
    "cross-env": "^5.1.6",
    "cz-customizable": "^5.2.0",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "ts-loader": "^3.5.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.26",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^2.7.2",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
